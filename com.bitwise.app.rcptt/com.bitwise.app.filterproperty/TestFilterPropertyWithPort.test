        get-editbox -after [get-label Phase] | set-text 1
        get-button Edit -after [get-label "Operation Fields"] | click
    }
}
with [get-window "Filter Property"] {
    get-button Add | click
    with [get-table] {
        get-cell 0 0 | mouse down -count 1
        activate-cell-edit
        with [get-editbox] {
            set-text Filter
            key-type "TRAVERSE_TAB_NEXT"
        }
        apply-cell-edit -deactivate
    }
    with [get-button Add] {
        key-type "TRAVERSE_TAB_NEXT"
        click
    }
    with [get-table] {
        get-cell 1 0 | mouse down -count 1
        activate-cell-edit
        get-editbox | set-text "filter&"
        apply-cell-edit -deactivate
        //select "" | double-click
        get-cell 1 0 | mouse down -count 1
        activate-cell-edit
        get-editbox | set-text filter01
        apply-cell-edit -deactivate
        wait -ms 1000
    }
}
