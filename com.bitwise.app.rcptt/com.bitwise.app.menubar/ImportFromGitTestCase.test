Content-Type: text/ecl
Entry-Name: .content

get-menu "File/Import" | click
with [get-window Import] {
    get-tree | select "Git/Projects from Git"
    get-button "Next >" | click
}
with [get-window "Import Projects from Git"] {
    get-tree | select URI
    get-button "Next >" | click
    with [get-group Location | get-editbox -after [get-label "URI:"]] {
        set-text "git@gitlab:bhs-ui/bhs-ui.git"
    }
    get-button "Next >" | click
    get-button "Deselect All" | click
    get-tree | get-item master | check
    get-button "Next >" | click
}
set-dialog-result File "platform:/resource/bhs-ui"
with [get-window "Import Projects from Git"] {
    get-group Destination | get-button Browse | click
    get-button "Next >" | click
}
with [get-window "Cloning from git@gitlab:bhs-ui/bhs-ui.git"] {
    get-button "Next >" | click
    get-button "Deselect All" | click
    get-tree 
        | get-item "com.bitwise.app.aggregator.*" 
        | check
    get-button Finish | click
}
get-view "Project Explorer" | get-tree | select "com.bitwise.app.aggregator [bhs-ui master]" | get-menu Delete | click
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | check
    get-button OK | click
}
