Content-Type: text/ecl
Entry-Name: .content

get-menu "File/Import" | click
with [get-window Import] {
    get-tree | select "Git/Projects from Git"
    get-button "Next >" | click
}
with [get-window "Import Projects from Git"] {
    get-tree | select URI
    get-button "Next >" | click
    with [get-group Location | get-editbox -after [get-label "URI:"]] {
         set-text "https://github.com/hmkcode/java"
    }
    with [get-button "Next >"] {
        click
        click
    }
}
set-dialog-result File "platform:/resource/import"
with [get-window "Import Projects from Git"] {
    get-group Destination | get-button Browse | click
    get-button "Next >" | click
}
with [get-window "Cloning from https://github.com/hmkcode/java"] {
    get-button "Next >" | click
    get-button Finish | click
}
get-view "Project Explorer" | get-tree | select "simple-junit [import master]" | get-menu Delete | click
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | check
    get-button OK | click
}
