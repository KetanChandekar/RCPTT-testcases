get-menu "File/Import" | click
with [get-window Import] {
    get-tree | select "General/Existing Projects into Workspace"
    get-button "Next >" | click
}
set-dialog-result Folder "platform:/resource/importProject"
with [get-window Import] {
    get-button "Browse..." | click
    get-button Finish | click
}

get-view "Project Explorer" | get-tree | select importProject | get-menu Delete | click
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | check
    get-button OK | click
}
